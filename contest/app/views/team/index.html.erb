<div class="container-fluid">
    <div class="row">
    <% if !@teams.blank? %>
          <% for team in @teams %>
            <div class="col-md-3">
                <div class="team-type-background-border team-type-<%= team.id %>">
                  <div class="team-link team-key" id="<%= team.id%>"><%= team.keys%></div>
                </div>
                <div class="team-link" id="<%= team.id%>"><%= team.points%></div>
            </div>
          <% end %>
      <% end %>
    </div>
  </div>